@prefix map: <#> .
@prefix db: <> .
@prefix vocab: <vocab/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#> .
@prefix jdbc: <http://d2rq.org/terms/jdbc/> .
@prefix corago: <http://corago.unibo.it/sm/> .
@prefix frbroo: <http://erlangen-crm.org/efrbroo/> .
@prefix crm: <http://erlangen-crm.org/current/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace/#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix d2r: <http://sites.wiwiss.fu-berlin.de/suhl/bizer/d2r-server/config.rdf#> .
@prefix bf: <http://id.loc.gov/ontologies/bibframe/> .
@prefix ud: <http://universaldependencies.org/u/dep#>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.

# Realizzato da: Paolo Bonora
# Versione del: 29/10/2020

<> a d2r:Server;
  d2r:sparqlTimeout 0;
  d2r:pageTimeout 5;
 .

map:database a d2rq:Database;
	d2rq:jdbcDriver "sun.jdbc.odbc.JdbcOdbcDriver";
	d2rq:jdbcDSN "jdbc:odbc:ArchivioTCBo";
.

map:Tipo a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:class frbroo:E55_Type;
	d2rq:uriPattern "TCBo/TIP/@@inventario.tipo1|urlify@@";
	d2rq:condition "inventario.tipo1<>''";
.
map:Inv a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:class frbroo:F5_Item;
	d2rq:uriPattern "TCBo/INV/@@inventario.num_inv@@";
.
map:LabelInv a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Inv;
	d2rq:property rdfs:label;
	d2rq:column "inventario.num_inv";
	d2rq:condition "inventario.num_inv<>''";
.
map:Spett a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:class frbroo:F25_Performance_Plan;
	d2rq:uriPattern "TCBo/SPETT/@@inventario.anno|urlify@@/@@inventario.titolo1|urlify@@";
	d2rq:condition "inventario.titolo1<>''";
.
map:LabelSpett a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Spett;
	d2rq:property rdfs:label;
	d2rq:sqlExpression  "inventario.titolo1 & '-' & inventario.anno";
.
map:Opera a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:class frbroo:F1_Work;
	d2rq:uriPattern "TCBo/OPERA/@@inventario.titolo1|urlify@@";
	d2rq:condition "inventario.titolo1<>''";
.
map:LabelOpera a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Opera;
	d2rq:property rdfs:label;
	d2rq:sqlExpression  "inventario.titolo1";
.
map:LegameOperaSpett a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Opera;
	d2rq:property frbroo:R3_is_realised_in;
	d2rq:refersToClassMap map:Spett;
	d2rq:join "inventario.titolo1 = inventario.titolo1";
	d2rq:condition "inventario.titolo1<>''";
.
map:LegameSpettItem a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Inv;
	d2rq:property crm:P67_refers_to;
	d2rq:refersToClassMap map:Spett;
	d2rq:join "inventario.titolo1 = inventario.titolo1";
	d2rq:join "inventario.anno = inventario.anno";
	d2rq:condition "inventario.titolo1<>''";
.
map:LegameItemTipo a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Inv;
	d2rq:property crm:P2_has_type;
	d2rq:refersToClassMap map:Tipo;
	d2rq:join "inventario.tipo1 = inventario.tipo1";
	d2rq:condition "inventario.titolo1<>''";
.




