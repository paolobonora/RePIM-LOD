map:ItemURL a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "ITEM/MBIB_URL/@@SWT_DOC_RISM.ID_MBIBLIO@@";
	d2rq:condition "SWT_DOC_RISM.MBIB_URL <> ''";
