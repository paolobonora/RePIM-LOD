# Propriet√† di E39_Actor (e classi collegate)

map:ActorAppellation_label1 a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:ActorAppellation;
	d2rq:property rdfs:label;
	d2rq:pattern "@@RESPONS.NOME_R@@ @@RESPONS.PREFISSO@@ @@RESPONS.COGNOME_R@@";
	d2rq:condition "RESPONS.COGNOME_R <> '' AND RESPONS.NOME_R <>'' AND RESPONS.PREFISSO <>''";
.

map:ActorAppellation_label2 a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:ActorAppellation;
	d2rq:property rdfs:label;
	d2rq:pattern "@@RESPONS.NOME_R@@ @@RESPONS.COGNOME_R@@";
	d2rq:condition "RESPONS.COGNOME_R <> '' AND RESPONS.NOME_R <>'' AND ISNULL(RESPONS.PREFISSO)";
.

map:ActorAppellation_label3 a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:ActorAppellation;
	d2rq:property rdfs:label;
	d2rq:pattern "@@RESPONS.COGNOME_R@@";
	d2rq:condition "RESPONS.COGNOME_R <> '' AND ISNULL(RESPONS.NOME_R) AND ISNULL(RESPONS.PREFISSO)";
.

map:ActorAppellation_label4 a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:ActorAppellation;
	d2rq:property rdfs:label;
	d2rq:pattern "@@RESPONS.PREFISSO@@ @@RESPONS.COGNOME_R@@";
	d2rq:condition "RESPONS.COGNOME_R <> '' AND ISNULL(RESPONS.NOME_R) AND RESPONS.PREFISSO <> ''";
.

map:PersonTypeTitleP2 a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Responsabile;
	d2rq:property crm:P2_has_type;
	d2rq:refersToClassMap map:PersonTypeTitle;
	d2rq:condition "RESPONS.QUALIFICA <> ''";
.

map:PersonTypeGenderP2 a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Responsabile;
	d2rq:property crm:P2_has_type;
	d2rq:refersToClassMap map:PersonTypeGender;
	d2rq:condition "RESPONS.SESSO <> ''";
.

map:BCWorkAuthorRole a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Brano;
	d2rq:property corago:CPBC29_work_author_role;
	d2rq:uriPattern "RESPONS/TITLE&@@RESPONS.COD_RESP|urlify@@";
	d2rq:condition "RESPONS.QUALIFICA <> ''";
	d2rq:join "RESPONS.COD_RESP = BRA_AUT.COD_RESP";
	d2rq:join "BRANI.COD_BRANO = BRA_AUT.COD_BRANO";
.

#Gestione forma primaria
map:Responsabile_Appellation a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Responsabile;
	d2rq:property crm:P131_is_identified_by;
	#d2rq:uriPattern "RESPONS/APPL/@@RESPONS.COD_RESP|urlify@@";
	d2rq:refersToClassMap map:ActorAppellation;
	d2rq:join "RESPONS.COD_RESP = RESPONS.COD_RESP";
.

# Gestione delle altre forme del nome in base a COD_RESP0
map:Responsabile_AppellationAltreForme a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Responsabile;
