map:ItemURLLabel1 a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:ItemURL;
	d2rq:property rdfs:label;
	#d2rq:join "SWT_DOC_RISM.ID_RISM = RISM.ID_RISM";
	d2rq:column "SWT_DOC_RISM.MBIB_URL";
	d2rq:condition "SWT_DOC_RISM.MBIB_URL like 'http*'";
