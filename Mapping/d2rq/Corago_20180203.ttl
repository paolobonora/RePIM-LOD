	d2rq:condition "RESPONS.COD_RESP <> RESPONS.COD_RESP0";
	#d2rq:refersToClassMap map:ActorAppellation;
	#d2rq:join "RESPONS.COD_RESP = RESPONS.COD_RESP0";
.
map:Responsabile_AppellationAltreFormeTipo a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "TADE/RES/PARENT_FORM";
	d2rq:class crm:E55_Type;
.
map:Responsabile_AppellationAltreFormeTipoLabel a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Responsabile_AppellationAltreFormeTipo;
	d2rq:property rdfs:label;
	d2rq:constantValue "forma principale del nome";
.
map:LegameResponsabile_AppellationAltreForme a d2rq:PropertyBridge;
	#d2rq:uriPattern "RESPONS/APPL/@@RESPONS.COD_RESP|urlify@@";
	d2rq:belongsToClassMap map:ActorAppellation;
	d2rq:condition "RESPONS.COD_RESP = RESPONS.COD_RESP0";
	d2rq:property crm:P2_has_type;
	d2rq:refersToClassMap map:Responsabile_AppellationAltreFormeTipo;
.

