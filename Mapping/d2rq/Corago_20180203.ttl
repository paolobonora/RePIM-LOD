	d2rq:dataStorage map:database;
	d2rq:uriPattern "DOCUMENTI/EXP/@@DOCUMENTI.ID_DOC|urlify@@";
	#d2rq:uriColumn  "DOCUMENTI.ID_DOC";
	d2rq:condition "NOT EXISTS (SELECT ID_DOC_BRA FROM SWT_DOC_BRA WHERE SWT_DOC_BRA.ID_DOC = DOCUMENTI.ID_DOC)";
